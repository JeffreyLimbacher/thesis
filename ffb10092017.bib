@article{AlekseenkoNguyenWood2015,
title = "A Deterministic-Stochastic Method for Computing the Boltzmann Collision Integral in $\mathcal{O}(MN)$ Operations",
journal = "submitted to Kinetic and Related Models",
volume = "",
number = "",
pages = "30p",
year = "2015",
note = "",
issn = "",
doi = "",
url = "",
author = "A. Alekseenko and T. Nguyen and A. Wood",
keywords = "Boltzmann collision operator",
keywords = "Stochastic solution",
keywords = "Discontinuous Galerkin methods ",
abstract = "Abstract We developed and implemented a numerical algorithm for evaluating the Boltzmann collision operator
with $O(MN)$ operations, where $N$ is the number of the discrete velocity points and $M<N$. The algorithm 
uses nodal-discontinuous Galerkin discretizations of the collision operator on uniform grids and a bilinear 
convolution form of the Galerkin projection of the collision operator. Efficiency of the algorithms is achieved 
by applying singular value decomposition compression of the discrete collision kernel and by approximating the kinetic 
solution by a sum of Maxwellian streams using a stochastic likelihood maximization algorithm. Accuracy of the 
method is established on solutions to the problem of spatially 
homogeneous relaxation. "
}

