@article{AlekseenkoJosyula2012a,
title = "Deterministic solution of the spatially homogeneous Boltzmann equation using discontinuous Galerkin discretizations in the velocity space ",
journal = "Journal of Computational Physics ",
volume = "272",
number = "0",
pages = "170 - 188",
year = "2014",
note = "",
issn = "0021-9991",
doi = "http://dx.doi.org/10.1016/j.jcp.2014.03.031",
url = "http://www.sciencedirect.com/science/article/pii/S0021999114002186",
author = "A. Alekseenko and E. Josyula",
keywords = "Boltzmann collision operator",
keywords = "Deterministic solution",
keywords = "Discontinuous Galerkin methods ",
abstract = "Abstract We present a new deterministic approach for the solution of the spatially homogeneous Boltzmann kinetic equation based on nodal discontinuous Galerkin (DG) discretizations in the velocity space. In the new approach the collision operator has the form of a bilinear operator with a pre-computed kernel; its evaluation requires O ( n 5 ) operations at every point of the phase space where n is the number of degrees of freedom in one velocity dimension. The method is generalized to any molecular potential. Results of numerical simulations are presented for the problem of spatially homogeneous relaxation for the hard spheres potential. Comparison with the method of Direct Simulation Monte Carlo showed excellent agreement. "
}

